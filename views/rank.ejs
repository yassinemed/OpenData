<!-- views/index.ejs -->
<!doctype html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <title>Ranking</title>
   
</head>
<body>

data : <%= JSON.stringify(data.json)%>
<ul>
    <% for(var i=0; i<data.length;i++) { %>
    <li>
        <a href="<% data[i].url %>"></a>
        <h1><% data[i].name %></h1>
        <p> <% data[i].species %> </p>
        <p> <% data[i].gender %> </p>
        <p> <% data[i].house %> </p>
        <p> <% data[i].dateOfBirth %> </p>
        <p> <% data[i].yearOfBirth %> </p>
        <p> <% data[i].ancestry %> </p>
        <p> <% data[i].eyeColour %> </p>
        <p> <% data[i].hairColour %> </p>
        <p> <% data[i].patronus %> </p>
        <p> <% data[i].actor %> </p>
        <p> <% data[i].quote %> </p>
    </li>
    <% } %>
</ul>
</body>
</html>